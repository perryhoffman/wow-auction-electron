{
  "name": "autotioneer",
  "productName": "WoW Much Remote",
  "version": "0.1.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron ./app",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.alexcheuk.wowremote",
    "app-category-type": "public.wowremote",
    "files": ["dist/**/*", "electron.js", "package.json"]
  },
  "devDependencies": {
    "electron": "^1.3.4",
    "cross-env": "^2.0.1",
    "devtron": "^1.3.0",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
