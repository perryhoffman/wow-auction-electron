{
  "name": "auctionpro",
  "productName": "AuctionPro",
  "version": "0.1.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron ./app",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "clean:win": "rm -rf dist/win && rm -rf dist/win-unpacked",
    "dist": "npm run clean:win && build"
  },
  "build": {
    "appId": "com.alexcheuk.wowremote",
    "app-category-type": "public.wowremote",
    "files": ["dist/**/*", "electron.js", "package.json"],
    "win": {
    }
  },
  "devDependencies": {
    "electron": "^1.3.4",
    "cross-env": "^2.0.1",
    "devtron": "^1.3.0"
  }
}
